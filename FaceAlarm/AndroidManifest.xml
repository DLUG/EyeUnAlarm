<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.dlug.android.facealarm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/bluedol"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstGroup"></activity>
        <activity android:name=".SecondGroup"></activity>
        <activity android:name=".AlarmData"></activity>
        <activity android:name=".ShowAlarm"></activity>
        <activity android:name="MyData"></activity>
        <activity android:name=".AlarmSet"></activity>
        <activity android:name="NavigationActivity"></activity>
        <activity android:name="ViewCounselGroup"></activity>
        <activity android:name="AlarmAdapter"></activity>
        <activity android:name="org.dlug.android.facealarm.AlarmSetAdapter"></activity>
        <receiver android:name="org.dlug.android.facealarm.AlarmReceiver"></receiver>
    </application>

</manifest>